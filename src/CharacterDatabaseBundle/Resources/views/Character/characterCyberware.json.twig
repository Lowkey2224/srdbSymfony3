[
    {% for item in cyberware %}
    {
        "name": "{{ item.cyberware.cyberware.name }}",
        "description": "{{ item.cyberware.cyberware.description }}",
        "level": {{ item.cyberware.level}},
        "effect": "{{ item.cyberware.effect}}",
        "quality": {
            "id": {{ item.quality}},
            "name": "{{ item.qualityName }}",
            "code": "{{  item.qualityCode }}"
        },
        "cost": {{ item.getQualityFactor*item.cyberware.cost}}
    }
    {% if not loop.last %},
    {% endif %}
    {% endfor %}
]